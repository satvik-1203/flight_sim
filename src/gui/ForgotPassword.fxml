<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #d1d5db;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ForgotPasswordController">
   <children>
      <Label layoutX="45.0" layoutY="39.0" text="Forgot Password">
         <font>
            <Font name="American Typewriter" size="28.0" />
         </font>
      </Label>
      <TextField fx:id="usernameT" layoutX="45.0" layoutY="151.0" prefHeight="33.0" prefWidth="192.0" promptText="Username" />
      <TextField fx:id="answerT" layoutX="286.0" layoutY="151.0" prefHeight="33.0" prefWidth="192.0" promptText="Answer" />
      <ComboBox fx:id="securityQuestionC" layoutX="45.0" layoutY="95.0" prefHeight="33.0" prefWidth="290.0" promptText="Security Question" />
      <Button layoutX="190.0" layoutY="238.0" mnemonicParsing="false" onAction="#done" prefHeight="26.0" prefWidth="74.0" text="Done" />
      <Button layoutX="45.0" layoutY="238.0" mnemonicParsing="false" onAction="#mainMenu" prefHeight="26.0" prefWidth="106.0" text="Main Menu" />
   </children>
